;; Additional restriction on arities where it makes sense

(def __=__ =)
(defn =
  ^Boolean [x y & more]
  (apply __=__ x y more))

(def __not=__ not=)
(defn not=
  ^Boolean [x y & more]
  (apply __not=__ x y more))

(def __<__ <)
(defn <
  ^Boolean [^Number x ^Number y & more]
  (apply __<__ x y more))

(def __>__ >)
(defn >
  ^Boolean [^Number x ^Number y & more]
  (apply __>__ x y more))

(def __<=__ <=)
(defn <=
  ^Boolean [^Number x ^Number y & more]
  (apply __<=__ x y more))

(def __>=__ >=)
(defn >=
  ^Boolean [^Number x ^Number y & more]
  (apply __>=__ x y more))

(def __==__ ==)
(defn ==
  ^Boolean [^Number x ^Number y & more]
  (apply __==__ x y more))
